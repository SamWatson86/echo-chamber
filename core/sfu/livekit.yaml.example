# LiveKit config (example for native)
# Copy to livekit.yaml and fill in keys.

port: 7880
log_level: info

rtc:
  tcp_port: 7881
  port_range_start: 55000
  port_range_end: 55100
  use_external_ip: true

redis:
  address: 127.0.0.1:6379

# Replace with your own key/secret
keys:
  LK_API_KEY: LK_API_SECRET
